#include<iostream>
using namespace std;

int main()
{
    int min,max,n;
    cin>>n;
    int arr[n];
    for(int x=0;x<n;x++)
    {
        cin>>arr[x];
    }
    for(int x=0;x<n;x++)
    {
        if(arr[x]<min) min=arr[x];
        if(arr[x]>max) max=arr[x];
        
    }
    cout<<"Minimum element is\t"<<min;
    cout<<"\nMaximum element is\t"<<max;
    
    
    return 0;
    
    
}
